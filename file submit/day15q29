// Q29: Write a program to calculate the factorial of a number.

#include<stdio.h>

int main(void)
{
    int fact = 1, i, num;

    printf("Enter a number to get its factorial: ");
    scanf("%d", &num);

    for(i = 1; i <= num; i++)
    {
        fact *= i;
    }

    printf("%d", fact);

    return 0;
}